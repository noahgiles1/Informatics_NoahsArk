package application;

import java.util.Arrays;
import java.util.List;

public class JSONLD {
	private String context;
	
	private String type;
	
	private int TotalRecovered;
	
	private int TotalConfirmed;
	
	private int TotalDeaths;
	
	private String datePosted;
	
	private String name;
	
	
	private JSONLD spatialCoverage;
	
	public String getContext() {
		return context;
	}

	public void setContext(String context) {
		this.context = context;
	}
	
	public int getTotalRecovered ()
	   {
	       return TotalRecovered;
	   }

	public void setTotalRecovered (int i)
	   {
	       this.TotalRecovered = i;
	   }

	public int getTotalConfirmed() {
		return TotalConfirmed;
	}

	public void setTotalConfirmed(int i) {
		this.TotalConfirmed = i;
	}

	public int getTotalDeaths() {
		return TotalDeaths;
	}

	public void setTotalDeaths(int i) {
		this.TotalDeaths = i;
	}

	public String getDatePosted() {
		return datePosted;
	}

	public void setDatePosted(String datePosted) {
		this.datePosted = datePosted;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}


	public JSONLD getSpatialCoverage() {
		return spatialCoverage;
	}
	
	public void setSpatialCoverage(JSONLD type) {
		this.spatialCoverage = type;
	}
	
	
}
